<view class="new-users"><view class="head"><view class="user-card"><view class="bg"></view><view class="user-info"><block wx:if="{{userInfo.avatar}}"><image class="avatar" src="{{userInfo.avatar}}" data-event-opts="{{[['tap',[['goEdit']]]]}}" bindtap="__e"></image></block><block wx:else><image class="avatar" src="/static/images/f.png" mode data-event-opts="{{[['tap',[['goEdit']]]]}}" bindtap="__e"></image></block><view class="info"><block wx:if="{{!userInfo.uid}}"><view data-event-opts="{{[['tap',[['openAuto',['$event']]]]]}}" class="name" bindtap="__e">请点击授权</view></block><block wx:if="{{userInfo.uid}}"><view class="name">{{''+userInfo.nickname+''}}<block wx:if="{{userInfo.vip}}"><view class="vip"><image src="{{userInfo.vipIcon}}" alt></image><view class="vip-txt" style="margin-left:10rpx;">{{userInfo.vipName}}</view></view></block></view></block><block wx:if="{{userInfo.phone}}"><view data-event-opts="{{[['tap',[['goEdit']]]]}}" class="num" bindtap="__e"><view class="num-txt">{{"ID："+userInfo.uid}}</view><view class="icon"><image src="/static/images/edit.png" mode></image></view></view></block><block wx:if="{{!userInfo.phone&&isLogin}}"><view data-event-opts="{{[['tap',[['bindPhone',['$event']]]]]}}" class="phone" bindtap="__e">绑定手机号</view></block></view></view><view class="num-wrapper"><view data-event-opts="{{[['tap',[['goMenuPage',['/pages/users/user_money/index']]]]]}}" class="num-item" bindtap="__e"><text class="num">{{userInfo.nowMoney?$root.g0:0}}</text><view class="txt">余额</view></view><view data-event-opts="{{[['tap',[['goMenuPage',['/pages/users/user_integral/index']]]]]}}" class="num-item" bindtap="__e"><text class="num">{{userInfo.integral||0}}</text><view class="txt">积分</view></view><view data-event-opts="{{[['tap',[['goMenuPage',['/pages/users/user_coupon/index']]]]]}}" class="num-item" bindtap="__e"><text class="num">{{userInfo.couponCount||0}}</text><view class="txt">优惠券</view></view></view><view data-event-opts="{{[['tap',[['goSignIn',['$event']]]]]}}" class="sign" bindtap="__e">签到</view></view><view class="order-wrapper"><view class="order-hd flex"><view class="left">订单中心</view><navigator class="right flex" hover-class="none" url="/pages/users/order_list/index" open-type="navigate">查看全部<text class="iconfont icon-xiangyou"></text></navigator></view><view class="order-bd"><block wx:for="{{orderMenu}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><navigator class="order-item" hover-class="none" url="{{item.url}}"><view class="pic"><image src="{{item.img}}" mode></image><block wx:if="{{item.num>0}}"><text class="order-status-num">{{item.num}}</text></block></view><view class="txt">{{item.title}}</view></navigator></block></block></view></view></view><block wx:if="{{imgUrls.length>0}}"><view class="slider-wrapper"><swiper indicator-dots="true" autoplay="{{autoplay}}" circular="{{circular}}" interval="{{interval}}" duration="{{duration}}" indicator-color="rgba(255,255,255,0.6)" indicator-active-color="#fff"><block wx:for="{{imgUrls}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item><navigator class="slide-navigator acea-row row-between-wrapper" url="{{item.url}}" hover-class="none"><image class="slide-image" src="{{item.pic}}"></image></navigator></swiper-item></block></block></swiper></view></block><view class="user-menus" style="margin-top:20rpx;"><block wx:for="{{MyMenus}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{!(item.url=='/pages/service/index'||item.url=='/pages/users/user_spread_user/index'&&!userInfo.isPromoter)}}"><navigator class="item" url="{{item.url}}" hover-class="none"><view class="left"><image src="{{item.pic}}"></image><text>{{item.name}}</text></view><view class="iconfont icon-xiangyou"></view></navigator></block></block></block><button class="item" open-type="contact" hover-class="none"><view class="left"><image src="/static/images/user_menu08.png"></image><text>联系客服</text></view><view class="iconfont icon-xiangyou"></view></button></view><view style="height:50rpx;"></view><authorize vue-id="5c2bbbc2-1" isAuto="{{isAuto}}" isShowAuth="{{isShowAuth}}" data-event-opts="{{[['^onLoadFun',[['onLoadFun']]],['^authColse',[['authColse']]]]}}" bind:onLoadFun="__e" bind:authColse="__e" bind:__l="__l"></authorize></view>