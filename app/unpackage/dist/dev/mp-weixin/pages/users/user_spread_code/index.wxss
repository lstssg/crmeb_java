page {
  background-color: #a3a3a3 !important;
}
.canvas canvas {
  position: fixed;
  z-index: -5rpx;
  opacity: 0;
}
.distribution-posters swiper {
  width: 100%;
  height: 1000rpx;
  position: relative;
  margin-top: 40rpx;
}
.distribution-posters .slide-image {
  width: 100%;
  height: 100%;
  margin: 0 auto;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
}
.distribution-posters .slide-image.active {
  -webkit-transform: none;
          transform: none;
  -webkit-transition: all 0.2s ease-in 0s;
  transition: all 0.2s ease-in 0s;
}
.distribution-posters .slide-image.quiet {
  -webkit-transform: scale(0.83333);
          transform: scale(0.83333);
  -webkit-transition: all 0.2s ease-in 0s;
  transition: all 0.2s ease-in 0s;
}
.distribution-posters .keep {
  font-size: 30rpx;
  color: #fff;
  width: 600rpx;
  height: 80rpx;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  text-align: center;
  line-height: 80rpx;
  margin: 38rpx auto;
}
.distribution-posters .preserve {
  color: #fff;
  text-align: center;
  margin-top: 38rpx;
}
.distribution-posters .preserve .line {
  width: 100rpx;
  height: 1px;
  background-color: #fff;
}
.distribution-posters .preserve .tip {
  margin: 0 30rpx;
}

